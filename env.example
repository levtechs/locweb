# Google Maps API configuration
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# OpenCode API configuration
OPENCODE_API_KEY=your_opencode_api_key_here
OPENCODE_HOST=127.0.0.1
OPENCODE_PORT=4096

# Stripe configuration
# Get keys from https://dashboard.stripe.com/apikeys
STRIPE_SECRET_KEY=sk_test_your_secret_key
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key

# Stripe Price IDs (server-side only - these are NOT exposed to frontend)
# Create two products in Stripe Dashboard:
# 1. One-time product for setup fee (e.g., $45)
# 2. Recurring product for monthly hosting (e.g., $5/month)
# Get price IDs from each product (starts with 'price_')
STRIPE_SETUP_FEE_PRICE_ID=price_your_setup_fee
STRIPE_MONTHLY_PRICE_ID=price_your_monthly_subscription
